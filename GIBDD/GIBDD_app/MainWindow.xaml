<Window x:Class="GIBDD_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GIBDD_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Background="#b5d5ca"></Grid>
        <Image Source="Resources/gibdd_logo.png" HorizontalAlignment="Left" Margin="5"></Image>
        <TextBlock Text="GIBDD" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Grid Grid.Row="2" Background="#d1eefc"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name ="MainFrame"></Frame>
        <Button Content="Go Back" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"></Button>
        <Image Panel.ZIndex="-4" Source="Resources/фон2.jpg" Margin="-156,-76,-130,-53" Grid.RowSpan="3"/>
    </Grid>
</Window>
